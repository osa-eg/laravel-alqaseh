{
    "name": "osa-eg/laravel-alqaseh",
    "description": "Laravel package for Al Qaseh payment gateway integration",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Osama Saad",
            "email": "osaad96eg@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Osama\\AlQaseh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osama\\AlQaseh\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osama\\AlQaseh\\AlQasehServiceProvider"
            ],
            "aliases": {
                "AlQaseh": "Osama\\AlQaseh\\AlQasehFacade"
            }
        }
    }
}